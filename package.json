{
  "name": "ithelpdesk-da-codex-atk",
  "version": "1.0.0",
  "description": "IT Helpdesk M365 Copilot Declarative Agent with ServiceNow integration",
  "type": "module",
  "scripts": {
    "mock:start": "prism mock ./.tmp/openapi.local.yaml --port $API_PORT",
    "spec:serve": "http-server -p $SPEC_SERVER_PORT -c-1 ./.tmp",
