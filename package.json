    "seed:examples": "node ./scripts/seed/seed-examples.mjs",
    "dev": "echo \"Replace with your agent run command (e.g., 'atk preview --env local')\""
  },
  "keywords": [
    "m365",
    "copilot",
    "declarative-agent",
    "servicenow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@stoplight/prism-cli": "^5.9.0",
    "csv-parse": "^5.5.0",
    "http-server": "^14.1.1",
    "js-yaml": "^4.1.0"
  }
}
